{
  "name": "aeo.js",
  "version": "0.0.1",
  "description": "Answer Engine Optimization for the modern web. Make your site discoverable by AI crawlers and LLMs.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.mjs",
      "require": "./dist/next.js"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.mjs",
      "require": "./dist/astro.js"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    },
    "./widget": {
      "types": "./dist/widget.d.ts",
      "import": "./dist/widget.mjs",
      "require": "./dist/widget.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "aeo",
    "answer-engine-optimization",
    "llm",
    "ai",
    "seo",
    "llms-txt",
    "robots-txt",
    "ai-crawlers",
    "structured-data",
    "json-ld",
    "chatgpt",
    "claude",
    "perplexity",
    "searchgpt",
    "web-optimization",
    "vite",
    "webpack",
    "nextjs",
    "react",
    "vue",
    "angular",
    "svelte"
  ],
  "author": "Ruben Marcus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/multivmlabs/aeo.js"
  },
  "homepage": "https://aeojs.org",
  "bugs": {
    "url": "https://github.com/multivmlabs/aeo.js/issues"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@astrojs/astro": ">=3.0.0",
    "next": ">=13.0.0",
    "react": ">=17.0.0",
    "webpack": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "@astrojs/astro": {
      "optional": true
    },
    "next": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@types/minimatch": "^5.1.2",
    "minimatch": "^10.2.2"
  }
}
